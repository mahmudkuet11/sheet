{
    "name": "mahmud/sheet",
    "description": "Excel and CSV file reader",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mahmudur Rahman",
            "email": "mahmudkuet11@gmail.com",
            "homepage": "https://mahmud.live"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "box/spout": "^3.0"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Mahmud\\Sheet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mahmud\\Sheet\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "keywords": ["Excel", "CSV", "Sheet"],
    "require-dev": {
        "phpunit/phpunit": "^7"
    }
}
